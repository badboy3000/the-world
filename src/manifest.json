{
  "package": "com.application.demo",
  "name": "vue-hap-demo",
  "versionName": "1.0.0",
  "versionCode": "1",
  "minPlatformVersion": "101",
  "icon": "/assets/logo.png",
  "features": [{
    "name": "system.prompt"
  }, {
    "name": "system.router"
  }, {
    "name": "system.shortcut"
  }],
  "permissions": [{
    "origin": "*"
  }],
  "config": {
